<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
					xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<ResourceDictionary.MergedDictionaries>
		<ResourceDictionary Source="/Styles/ColorStyle.xaml" />
	</ResourceDictionary.MergedDictionaries>
	<SolidColorBrush x:Key="MainSolidColorBurshStyle"
					 Color="{StaticResource MainColor}" />
	<SolidColorBrush x:Key="SecondarySolidColorBurshStyle"
					 Color="{StaticResource SecondaryColor}" />
	<SolidColorBrush x:Key="TertiarySolidColorBurshStyle"
					 Color="{StaticResource TertiaryColor}" />
	<SolidColorBrush x:Key="WarnSolidColorBurshStyle"
					 Color="{StaticResource WarnColor}" />
	<SolidColorBrush x:Key="MainTextSolidColorBurshStyle"
					 Color="{StaticResource MainTextColor}" />
	<SolidColorBrush x:Key="SecondaryTextSolidColorBurshStyle"
					 Color="{StaticResource SecondaryTextColor}" />
	<SolidColorBrush x:Key="TertiaryTextSolidColorBurshStyle"
					 Color="{StaticResource TertiaryTextColor}" />
</ResourceDictionary>